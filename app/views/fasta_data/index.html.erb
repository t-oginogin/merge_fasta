<h1>Listing fasta_data</h1>

<table>
  <thead>
    <tr>
      <th>Filename</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fasta_data.each do |fasta_datum| %>
      <tr>
        <td><%= fasta_datum.filename %></td>
        <td><%= fasta_datum.data %></td>
        <td><%= link_to 'Show', fasta_datum %></td>
        <td><%= link_to 'Edit', edit_fasta_datum_path(fasta_datum) %></td>
        <td><%= link_to 'Destroy', fasta_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fasta datum', new_fasta_datum_path %>
<%= link_to 'merge', fasta_data_merge_path %>
